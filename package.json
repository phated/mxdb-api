{
  "name": "mxdb",
  "description": "GraphQL Gateway to access different features of MXDB.",
  "license": "MPL-2.0",
  "repository": "https://github.com/phated/mxdb-api",
  "scripts": {
    "start": "node -r esm src/index.js",
    "deploy": "now --docker --public && now alias"
  },
  "esy": {
    "build": "pesy",
    "release": {
      "releasedBinaries": [
        "mxdb-api.exe"
      ]
    },
    "sandboxEnv": {
      "PATH": "/Users/phated/brew/bin:$PATH"
    }
  },
  "buildDirs": {
    "library": {
      "require": [
        "caqti",
        "caqti-lwt",
        "caqti-driver-postgresql",
        "dataloader",
        "dataloader-lwt",
        "graphql-lwt",
        "console.lib"
      ],
      "name": "mxdb.lib",
      "namespace": "Mxdb"
    },
    "server": {
      "require": [
        "mxdb.lib",
        "digestif.c",
        "graphql-cohttp",
        "graphql-lwt",
        "cohttp-lwt-unix",
        "console.lib"
      ],
      "main": "RunServer",
      "name": "mxdb-api.exe"
    }
  },
  "dependencies": {
    "@esy-ocaml/reason": "*",
    "@opam/caqti": "1.1.0",
    "@opam/caqti-driver-postgresql": "1.1.0",
    "@opam/caqti-lwt": "1.0.0",
    "@opam/cohttp-lwt": "2.0.0",
    "@opam/cohttp-lwt-unix": "2.0.0",
    "@opam/dataloader": "*",
    "@opam/dataloader-lwt": "*",
    "@opam/digestif": "0.7.1",
    "@opam/dune": "1.6.0",
    "@opam/graphql": "0.9.0",
    "@opam/graphql-cohttp": "0.12.1",
    "@opam/graphql-lwt": "0.9.0",
    "@opam/graphql_parser": "0.9.0",
    "@opam/lwt": "4.2.0",
    "@reason-native/console": "^0.1.0",
    "ocaml": "~4.6.0",
    "pesy": "*",
    "refmterr": "*"
  },
  "devDependencies": {
    "@opam/merlin": "*",
    "now": "^11.5.0"
  },
  "resolutions": {
    "@opam/dataloader": "phated/ocaml-dataloader:dataloader.opam#079ae64",
    "@opam/dataloader-lwt": "phated/ocaml-dataloader:dataloader-lwt.opam#079ae64"
  },
  "now": {
    "alias": "graphql.metaxdb.com"
  }
}
